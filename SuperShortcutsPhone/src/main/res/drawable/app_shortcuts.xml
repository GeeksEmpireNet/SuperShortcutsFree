    <item
        android:id="@+id/three"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="3dp"
        android:top="10dp" />
    <item
        android:id="@+id/four"
        android:width="13dp"
        android:height="13dp"
        android:drawable="@drawable/ic_launcher"
        android:gravity="top|left"
        android:left="10dp"
        android:top="12dp" />
</layer-list>